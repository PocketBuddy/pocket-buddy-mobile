{
  "name": "pocket-buddy",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator='iPhone 14'",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier src --check",
    "format:fix": "prettier src --write",
    "type-check": "tsc",
    "sanity": "yarn type-check && yarn lint && yarn format",
    "pod-install": "cd ios && RCT_NEW_ARCH_ENABLED=1 bundle exec pod install && cd ..",
    "pod-install-prod": "cd ios && RCT_NEW_ARCH_ENABLED=1 NO_FLIPPER=1 bundle exec pod install && cd ..",
    "link-fonts": "npx react-native-asset"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-brands-svg-icons": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
